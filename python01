print(type(f))
print("this is the type", type(a))
print("this is the type", type(b))
print("this is the type", type(c))
print("this is the type", type(d))
print("this is the type", type(e))
# this is implicit typecasting
ab = 2.3
bc = 7
abc = ab + bc
print(abc)
print("type of abc", type(abc))
